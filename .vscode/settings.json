{
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.indentSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "files.insertFinalNewline": true,
    "gitdoc.enabled": true,
    "git.autofetch": true,
    "gitdoc.autoCommitDelay": 60000,
    "gitdoc.commitMessageFormat": "ccc, LLL d, y, h:mm a '+00:00'",
    "editor.rulers": [
        80,
        120
    ],
    "gitdoc.commitOnClose": true,
    "oracledevtools.formatterSettings": {
        "keywordCasing": "Uppercase",
        "identifierCasing": "No change"
    }
}
